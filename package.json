{
  "name": "zcash-gui",
  "version": "0.2.0",
  "description": "Zcash Daemon & Wallet GUI",
  "main": "main.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack-dev-server --mode development --open",
    "electron": "electron ./electron/electron.js",
    "build": "webpack --mode production",
    "bundle:profile:prepare": "rm -rf ./webpack-stats/stats.json && touch ./webpack-stats/stats.json",
    "bundle:profile": "webpack --profile --json > webpack-stats/stats.json",
    "bundle:analyze": "webpack-bundle-analyzer webpack-stats/stats.json",
    "analyze": "yarn bundle:profile:prepare && yarn bundle:profile && yarn bundle:analyze",
    "lint:precommit": "eslint ./app/",
    "lint:fix": "eslint ./app/ --fix",
    "flow:precommit": "glow",
    "flow:coverage": "flow-coverage-report -t html -i 'app/**/*.js' -x 'dist/*.js' --threshold 70",
    "flow:report": "yarn flow:coverage && cd ./flow-coverage && open index.html",
    "deploy:staging": "yarn build && cd dist && now && now alias venn-staging.now.sh"
  },
  "author": {
    "email": "andrerfneves@protonmail.com",
    "name": "Andr√© Neves",
    "url": "https://andrenev.es"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.69.0",
    "flow-coverage-report": "^0.5.0",
    "glow": "^1.2.2",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^4.3.3",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.7",
    "serve": "^9.1.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "autoprefixer": "^8.3.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "decamelize": "^2.0.0",
    "electron": "^1.4.1",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "qrcode.react": "^0.8.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^2.0.0-beta.6",
    "react-table": "^6.8.0",
    "react-tradingview-widget": "^1.2.3",
    "react-truncate": "^2.3.0",
    "react-typekit": "^1.1.3",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "stdrpc": "^1.0.0",
    "uuid": "^3.2.1",
    "zcash": "^0.2.0"
  },
  "pre-commit": [
    "lint:precommit",
    "flow:precommit"
  ]
}
